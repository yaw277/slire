{
  "name": "slire",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "publish:dry": "npm publish --dry-run",
    "release": "npm run clean && npm run build && npm test && npm publish --access public --provenance",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write"
  },
  "peerDependencies": {
    "@google-cloud/firestore": "^7 || ^8",
    "mongodb": "^5 || ^6"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "keywords": [
    "firestore",
    "mongodb",
    "repository",
    "data-access",
    "typescript",
    "node",
    "stream"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@testcontainers/mongodb": "^11.7.2",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.20",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "firebase-tools": "^14.23.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.2.5",
    "tsup": "^8.5.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.46.3"
  },
  "license": "MIT"
}
